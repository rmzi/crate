  },
  "keywords": [
    "music",
    "streaming",
    "pwa",
    "aws"
  ],
  "license": "MIT",
  "devDependencies": {
    "gray-matter": "^4.0.3",
    "javascript-obfuscator": "^5.3.0",
    "marked": "^17.0.3",
    "sharp": "^0.34.5"
  }
}
